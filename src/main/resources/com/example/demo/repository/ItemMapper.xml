<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.repository.ItemMapper">
<insert id="insert">
  Insert INTO item (id,name,condition_id,small_category_id,brand,price,shipping,description)
  values (#{id},#{name},#{conditionId},#{smallCategoryId},#{brand},#{price},#{shipping},#{description})
</insert>
</mapper>